{
    "name": "ronasit/laravel-telescope-extension",
    "description": "Laravel library for Telescope",
    "keywords": ["telescope", "laravel", "prune"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Vitaly Iskrin",
            "email": "iskrinvv@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^9.0|^10.0",
        "laravel/telescope": "^4.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "RonasIT\\TelescopeExtension\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RonasIT\\TelescopeExtension\\TelescopeExtensionServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}